
<%= form_with(model:student) do |form| %>
<div>
    <%= form.label :name, style:"display: block"  %>
    <%= form.text_field :name %>
</div>


<div>
    <%= form.label :surname, style:"display: block"  %>
    <%= form.text_field :surname %>
</div>

<div>
    <%= form.label :age, style:"display: block"  %>
    <%= form.text_field :age %>
</div>
</br>
<div>
    <%= form.submit %>
</div>

<% end %>